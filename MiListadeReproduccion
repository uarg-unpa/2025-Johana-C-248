public class MiListadeReproduccion {
    private Cancion[] elementos;
    private final int MAX = 100;
    private int frente;
    private int final;
    private int movible;

    private MiListadeReproduccion() {
        elementos() = new Cancion[MAX];
        frente = 0;
        final = 0;
        movible = 0;
    }

    public void seleccionarCancion(aux.lista) {
        aux.lista 
        colecciones[aux.indice].desencolar();
    }
}